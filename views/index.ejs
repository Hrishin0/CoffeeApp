    color: #6F4E37;
  }
  blockquote:before {
    display: block;
    padding-left: 10px;
    content: "\201C";
    font-size: 80px;
    position: absolute;
    left: -20px;
    top: -20px;
    color: #6F4E37;
  }

  blockquote cite {
    color: #6F4E37;
    font-size: 14px;
    display: block;
    margin-top: 5px;
  }
    
  blockquote cite:before {
    content: "\2014 \2009";
  }
  blockquote:after {
    font-family: Georgia, serif;
    position: absolute;
    float:right;
    font-size:6em;
    line-height: 1;
    right:0;
    bottom:-0.5em;
    content: "\201D";
}
title{
